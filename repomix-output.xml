package searchengine.services;

import org.springframework.stereotype.Service;
import searchengine.model.Site;
import searchengine.model.Status;
import searchengine.repository.PageRepository;
import searchengine.repository.SiteRepository;
import searchengine.task.SiteIndexerTask;

import java.time.LocalDateTime;
import java.util.HashSet;
import java.util.Set;
import java.util.concurrent.ForkJoinPool;
import java.util.concurrent.TimeUnit;

@Service
public class IndexingServiceImpl {

    private final SiteRepository siteRepository;
    private final PageRepository pageRepository;

    private volatile boolean indexing = false;
    private ForkJoinPool pool;

    public IndexingServiceImpl(SiteRepository siteRepository, PageRepository pageRepository) {
        this.siteRepository = siteRepository;
        this.pageRepository = pageRepository;
    }

    public synchronized void indexSite(String siteUrl, String siteName) {
        if (indexing) {
            throw new RuntimeException("Индексация уже запущена");
        }

        indexing = true;
        pool = new ForkJoinPool();

        // 1️⃣ Поиск или создание Site
        Site site = siteRepository.findByUrl(siteUrl).orElseGet(() -> {
            Site newSite = new Site();
            newSite.setUrl(siteUrl);
            newSite.setName(siteName);
            return newSite;
        });

        // 2️⃣ Установка статуса и сохранение сайта (обязательно!)
        site.setStatus(Status.INDEXING);
        site.setStatusTime(LocalDateTime.now());
        site.setLastError(null);

        siteRepository.save(site); // <<< ВАЖНО: сохранить перед работой с Page

        // 3️⃣ Теперь можно удалять страницы по сайту
        pageRepository.deleteAllBySite(site);

        Set<String> visited = new HashSet<>();

        try {
            pool.invoke(new SiteIndexerTask(siteUrl, site, pageRepository, visited));
            site.setStatus(Status.INDEXED);
        } catch (Exception e) {
            site.setStatus(Status.FAILED);
            site.setLastError(e.getMessage());
        } finally {
            site.setStatusTime(LocalDateTime.now());
            siteRepository.save(site);
            indexing = false;
        }
    }

    public synchronized void stopIndexing() {
        if (!indexing) {
            throw new RuntimeException("Индексация не запущена");
        }

        if (pool != null && !pool.isShutdown()) {
            pool.shutdownNow();

            try {
                pool.awaitTermination(5, TimeUnit.SECONDS);
            } catch (InterruptedException e) {
                Thread.currentThread().interrupt();
            }
        }

        indexing = false;
    }

    public boolean isIndexing() {
        return indexing;
    }
}
